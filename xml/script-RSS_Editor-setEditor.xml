<window>
	<defaultcontrol>10</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>0</posx>
		<posy>0</posy>
		<origin x="0" y="0" />
	</coordinates>
	<controls>
		<control type="group">
			<!--
				This is highly hackish, but the RSS Editor script never closes
				its own windows so they just stack up.. this hack is effectively
				trying to clear the screen in-between each instance of the window.
			-->
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>1980</width>
				<height>1140</height>
				<texture background="true">BlackBG.png</texture>
			</control>
			<control type="image">
				<include>Dimensions_Fullscreen</include>
				<texture background="true" fallback="Background.png">$INFO[Skin.String(CustomBackground)]</texture>
				<colordiffuse>BackgroundColor</colordiffuse>
			</control>
		</control>
		<control type="group">
			<include>Animation_OpenCloseFade</include>
			<posy>-165</posy>
			<!--<control type="label" id="2">
				<description>Header Label</description>
				<posx>460</posx>
				<posy>175</posy>
				<width>810</width>
				<height>38</height>
				<font>METF_TitleTextLarge</font>
				<textcolor>SettingsTitle</textcolor>
				<align>left</align>
				<aligny>center</aligny>
			</control>-->
			<control type="label" id="3">
				<description>Secondary Header Label</description>
				<posx>690</posx>
				<posy>262</posy>
				<width>1215</width>
				<height>57</height>
				<font>METF_TitleTextLarge</font>
				<textcolor>SettingsTitle</textcolor>
				<align>left</align>
				<aligny>center</aligny>
			</control>
			<control type="list" id="10">
				<description>Control Area</description>
				<posx>660</posx>
				<posy>360</posy>
				<width>1260</width>
				<height>570</height>
				<itemgap>0</itemgap>
				<pagecontrol>60</pagecontrol>
				<scrolltime>120</scrolltime>
				<onleft>18</onleft>
				<onright>60</onright>
				<itemlayout width="0" height="45">
					<control type="label">
						<posx>30</posx>
						<posy>0</posy>
						<width>825</width>
						<height>45</height>
						<aligny>center</aligny>
						<font>METF_DialogLarge</font>
						<textcolor>TextNF</textcolor>
						<selectedcolor>TextFO</selectedcolor>
						<label>$INFO[ListItem.Label]</label>
						<scroll>false</scroll>
					</control>
				</itemlayout>
				<focusedlayout width="0" height="63">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>885</width>
						<height>63</height>
						<aligny>center</aligny>
						<texture>ButtonFO.png</texture>
						<visible>Control.HasFocus(10) | Control.HasFocus(14) | Control.HasFocus(11)</visible>
					</control>
					<control type="label">
						<posx>30</posx>
						<posy>0</posy>
						<width>825</width>
						<height>63</height>
						<aligny>center</aligny>
						<scrollout>false</scrollout>
						<pauseatend>1800</pauseatend>
						<scrollspeed>40</scrollspeed>
						<scroll>true</scroll>
						<font>METF_DialogLargeBold</font>
						<textcolor>ButtonText</textcolor>
						<selectedcolor>TextFO</selectedcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<posx>1905</posx>
				<posy>90</posy>
				<width>27</width>
				<height>711</height>
				<visible allowhiddenfocus="true">Control.IsVisible(10) + Control.HasFocus(60)</visible>
				<texturesliderbackground></texturesliderbackground>
				<textureslidernibfocus>ScrollbarVnibFO.png</textureslidernibfocus>
				<onleft>10</onleft>
				<onright>noop</onright>
				<showonepage>true</showonepage>
				<orientation>vertical</orientation>
				<animation effect="fade" start="0" end="100" time="400" condition="Control.HasFocus(60)">Conditional</animation>
			</control>
			<control type="group" id="9000">
				<posx>0</posx>
				<posy>360</posy>
				<control type="button" id="19">
					<description>Cancel</description>
					<include>SettingsLabelLeft</include>
					<label>$LOCALIZE[222]</label>
					<onleft>10</onleft>
					<onright>10</onright>
					<onup>9001</onup>
					<ondown>18</ondown>
				</control>
				<control type="button" id="18">
					<description>Ok</description>
					<posy>57</posy>
					<include>SettingsLabelLeft</include>
					<label>$LOCALIZE[186]</label>
					<onleft>10</onleft>
					<onright>10</onright>
					<onup>19</onup>
					<ondown>9001</ondown>
				</control>
			</control>
			<control type="group" id="9001">
				<posx>0</posx>
				<posy>588</posy>
				<control type="button" id="11">
					<description>Change Set</description>
					<include>SettingsLabelLeft</include>
					<onleft>10</onleft>
					<onright>10</onright>
					<onup>9000</onup>
					<ondown>13</ondown>
				</control>
				<control type="button" id="13">
					<description>Add</description>
					<posy>57</posy>
					<include>SettingsLabelLeft</include>
					<label>$LOCALIZE[15019]</label>
					<onleft>10</onleft>
					<onright>10</onright>
					<onup>11</onup>
					<ondown>14</ondown>
				</control>
				<control type="button" id="14">
					<description>Remove</description>
					<posy>114</posy>
					<include>SettingsLabelLeft</include>
					<label>$LOCALIZE[1210]</label>
					<onleft>10</onleft>
					<onright>10</onright>
					<onup>13</onup>
					<ondown>9000</ondown>
				</control>
			</control>
		</control>
		<include>Global_Weather</include>
		<include>Global_Time</include>
		<control type="group">
			<posx>45</posx>
			<posy>15</posy>
			<include>Animation_OpenCloseFade</include>
			<control type="label">
				<width>1125</width>
				<height>45</height>
				<font>METF_Heading</font>
				<textcolor>GlobalHeading</textcolor>
				<align>left</align>
				<aligny>center</aligny>
				<label>$LOCALIZE[5]</label>
			</control>
			<control type="label" id="2">
				<posx>1830</posx>
				<width>675</width>
				<height>45</height>
				<font>METF_Heading</font>
				<textcolor>GlobalHeading</textcolor>
				<align>right</align>
				<aligny>center</aligny>
			</control>
		</control>
		<include>Global_MediaOverlay</include>
	</controls>
</window>
