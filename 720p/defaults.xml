<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<default type="label">
		<font>METF_ContextValue</font>
		<textcolor>TextNF</textcolor>
		<focusedcolor>TextFO</focusedcolor>
		<selectedcolor>TextFO</selectedcolor>
		<pulseonselect>false</pulseonselect>
		<scrollsuffix>    -    </scrollsuffix>
	</default>

	<default type="button">
		<align>center</align>
		<aligny>center</aligny>
		<font>METF_ContextValue</font>
		<textcolor>TextNF</textcolor>
		<focusedcolor>TextFO</focusedcolor>
		<selectedcolor>TextFO</selectedcolor>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="togglebutton">
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="radiobutton">
		<posx>0</posx>
		<posy>0</posy>
		<width>300</width>
		<height>40</height>
		<align>center</align>
		<aligny>center</aligny>
		<font>METF_ContextValue</font>
		<label>-</label>
		<textcolor>TextNF</textcolor>
		<focusedcolor>DialogTextFO</focusedcolor>
		<disabledcolor>TextDisabled</disabledcolor>
		<radiowidth>30</radiowidth>
		<radioheight>30</radioheight>
		<texturenofocus border="2">keyboard/ButtonNF.png</texturenofocus>
		<texturefocus border="5">keyboard/ButtonFO.png</texturefocus>
		<textureradiofocus>keyboard/RadioButtonFO.png</textureradiofocus>
		<textureradionofocus>keyboard/RadioButtonNF.png</textureradionofocus>
		<textureradioondisabled colordiffuse="40FFFFFF">keyboard/RadioButtonFO.png</textureradioondisabled>
		<textureradiooffdisabled colordiffuse="40FFFFFF">keyboard/RadioButtonNF.png</textureradiooffdisabled>
		<textoffsetx>20</textoffsetx>
		<pulseonselect>false</pulseonselect>
	</default>

	<default type="spincontrolex">
		<pulseonselect>false</pulseonselect>
		<textureupdisabled colordiffuse="40FFFFFF">subtitles/scroll-up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="40FFFFFF">subtitles/scroll-down.png</texturedowndisabled>
	</default>

	<default type="spincontrol">
		<pulseonselect>false</pulseonselect>
		<textureupdisabled colordiffuse="40FFFFFF">subtitles/scroll-up.png</textureupdisabled>
		<texturedowndisabled colordiffuse="40FFFFFF">subtitles/scroll-down.png</texturedowndisabled>
	</default>

	<default type="slider">
		<orientation>horizontal</orientation>
	</default>

	<default type="progress">
		<reveal>true</reveal>
		<texturebg>osd/osd_progress_back.png</texturebg>
		<lefttexture>osd/osd_progress_left.png</lefttexture>
		<midtexture>osd/osd_progress_mid.png</midtexture>
		<righttexture>osd/osd_progress_right.png</righttexture>
		<overlaytexture>-</overlaytexture>
		<colordiffuse>DialogProgressBar</colordiffuse>
	</default>
</includes>
